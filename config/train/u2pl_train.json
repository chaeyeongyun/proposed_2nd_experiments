{   
    "project_name":"Unet+CSP_num30_barlow",
    "wandb_logging":true,
    "num_classes":3,
    "pixel_to_label":{
        "0":0,
        "128":1,
        "255":2
    },
    "resize":512,
    "model":{
        "in_channels":3,
        "backbone":{
            "name":"vgg16",
            "pretrain_weights":"/content/drive/MyDrive/self_supervised/CWFID/vgg16_0/ckpoints/150ep.pth"  
        },
        "seg_head":{
            "name":"unet"
        }
    },
    "train":{
        "data_dir":"/content/data/semi_sup_data/CWFID/num30",
        "save_dir":"/content/drive/MyDrive/semi_sup_train/CWFID",
        "device":0,
        "num_epochs":400,
        "sup_only_epoch":1,
        "batch_size":4,
        "learning_rate":1e-4,
        "lr_scheduler":{
            "name":"warmuppoly",
            "warmup_epoch":0,
            "lr_power":0.9
        },
        "strong_aug":{
            "name":"cutmix",
            "ratio":0.2
        },
        "half":true,
        "init_weights":true,
        "bn_eps":1e-5,
        "bn_momentum":0.1,
        "criterion":"cross_entropy",
        "unsup_loss":{
            "weight":1,
            "drop_percent":20
        },
        "contrastive_loss":{
            "negative_high_entropy": true,
            "low_rank": 3,
            "high_rank": 20,
            "current_class_threshold": 0.3,
            "current_class_negative_threshold": 1,
            "unsupervised_entropy_ignore": 80,
            "low_entropy_threshold": 20,
            "num_negatives": 50,
            "num_queries": 256,
            "temperature": 0.5
        },
        "resume":null,
        "save_img":true,
        "save_txt":true,
        "save_graph":true,
        "save_csv":true,
        "wandb_config":["learning_rate", "batch_size", "num_epochs"],
        "wandb_log":["loss", "miou", "sup_loss_1", "sup_loss_2", "cps_loss", "crop_iou", "weed_iou", "back_iou"],
        "wandb_metrics":["loss", "miou"] 
    }
}